{
    "name": "namdh2/newscrud",
    "type": "library",
    "description": "An admin panel for news with categories and tags, using Backpack\\CRUD on Laravel 5.",
    "keywords": [
        "backpack",
        "NewsCRUD"
    ],
    "homepage": "https://github.com/dinhhoainam23894/backpack-newcurd",
    "license": "proprietary",
    "authors": [
        {
            "name": "Dinh hoai nam",
            "email": "dinhhoainam94@gmail.com"
        }
    ],
    "require": {
        "backpack/crud": "^3.0.6",
        "cviebrock/eloquent-sluggable": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\News\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\News\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Backpack\\NewsCRUD\\NewsCRUDServiceProvider"
            ]
        }
    }
}
